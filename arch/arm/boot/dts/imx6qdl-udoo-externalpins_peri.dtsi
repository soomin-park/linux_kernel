            MX6QDL_PAD_GPIO_8__GPIO1_IO08        0x80000000  // {{external-gpio-54}}
            MX6QDL_PAD_GPIO_7__GPIO1_IO07        0x80000000  // {{external-gpio-55}}
            >;
        };
//cndi peri LED
		pinctrl_periled: ledgrp {
			fsl,pins = <
				MX6QDL_PAD_DISP0_DAT22__GPIO5_IO16   0x80000000  // {{external-gpio-45}} peri LED0
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17   0x80000000  // {{external-gpio-46}} peri LED1	
				MX6QDL_PAD_EIM_D25__GPIO3_IO25       0x80000000  // {{external-gpio-47}} peri LED2
				MX6QDL_PAD_SD1_CLK__GPIO1_IO20       0x80000000  // {{external-gpio-2}} peri LED3
				MX6QDL_PAD_SD1_DAT0__GPIO1_IO16      0x80000000  // {{external-gpio-3}} peri LED4
//				MX6QDL_PAD_SD1_DAT1__GPIO1_IO17      0x80000000  // {{external-gpio-4}} peri LED5	
				MX6QDL_PAD_SD4_DAT1__GPIO2_IO09      0x80000000  // {{external-gpio-6}} peri LED5
				MX6QDL_PAD_SD4_DAT2__GPIO2_IO10      0x80000000  // {{external-gpio-7}} peri LED6
				MX6QDL_PAD_GPIO_1__GPIO1_IO01        0x80000000  // {{external-gpio-10}} peri LED7
			>;

		};
//cndi  text LCD
		pinctrl_peritextlcd: textgrp {
			fsl,pins = <
            MX6QDL_PAD_DISP0_DAT11__GPIO5_IO05   0x80000000  // {{external-gpio-27}}  (LCD_RS)
            MX6QDL_PAD_DISP0_DAT12__GPIO5_IO06   0x80000000  // {{external-gpio-28}}  (LCD_RW)
            MX6QDL_PAD_DISP0_DAT13__GPIO5_IO07   0x80000000  // {{external-gpio-29}}  (LCD_E)
            MX6QDL_PAD_DISP0_DAT14__GPIO5_IO08   0x80000000  // {{external-gpio-30}}  (LCD_D0)
            MX6QDL_PAD_DISP0_DAT15__GPIO5_IO09   0x80000000  // {{external-gpio-31}}  (LCD_D1)
            MX6QDL_PAD_DISP0_DAT16__GPIO5_IO10   0x80000000  // {{external-gpio-32}}  (LCD_D2)
            MX6QDL_PAD_DISP0_DAT17__GPIO5_IO11   0x80000000  // {{external-gpio-33}}  (LCD_D3)
            MX6QDL_PAD_DISP0_DAT18__GPIO5_IO12   0x80000000  // {{external-gpio-34}}  (LCD_D4)
            MX6QDL_PAD_DISP0_DAT19__GPIO5_IO13   0x80000000  // {{external-gpio-35}}  (LCD_D5)
            MX6QDL_PAD_DISP0_DAT20__GPIO5_IO14   0x80000000  // {{external-gpio-36}}  (LCD_D6)
            MX6QDL_PAD_DISP0_DAT21__GPIO5_IO15   0x80000000  // {{external-gpio-37}}  (LCD_D7)
			>;

		};
// cndi fnd
		pinctrl_perifnd: fndgrp {
			fsl,pins = <
            MX6QDL_PAD_DISP0_DAT6__GPIO4_IO27    0x80000000  // {{external-gpio-22}} FND_D0
            MX6QDL_PAD_DISP0_DAT7__GPIO4_IO28    0x80000000  // {{external-gpio-23}} FND_D1
            MX6QDL_PAD_EIM_A16__GPIO2_IO22       0x80000000  // {{external-gpio-38}} FND_D2
            MX6QDL_PAD_DISP0_DAT8__GPIO4_IO29    0x80000000  // {{external-gpio-24}} FND_D3
            MX6QDL_PAD_DISP0_DAT9__GPIO4_IO30    0x80000000  // {{external-gpio-25}} FND_D4
            MX6QDL_PAD_DISP0_DAT10__GPIO4_IO31   0x80000000  // {{external-gpio-26}} FND_D5
            MX6QDL_PAD_KEY_ROW1__GPIO4_IO09      0x80000000  // {{external-gpio-48}} FND_D6
            MX6QDL_PAD_KEY_COL1__GPIO4_IO08      0x80000000  // {{external-gpio-49}} FND_D7
            MX6QDL_PAD_CSI0_DAT4__GPIO5_IO22     0x80000000  // {{external-gpio-14}} FND_S5
            MX6QDL_PAD_CSI0_DAT16__GPIO6_IO02    0x80000000  // {{external-gpio-15}} FND_S4
            MX6QDL_PAD_CSI0_DAT14__GPIO6_IO00    0x80000000  // {{external-gpio-16}} FND_S3
            MX6QDL_PAD_CSI0_DAT15__GPIO6_IO01    0x80000000  // {{external-gpio-17}} FND_S2
            MX6QDL_PAD_CSI0_DAT12__GPIO5_IO30    0x80000000  // {{external-gpio-18}} FND_S1
            MX6QDL_PAD_CSI0_DAT13__GPIO5_IO31    0x80000000  // {{external-gpio-19}} FND_S0
			>;
		};
// cndi buzzer
		pinctrl_peribuzzer: buzzergrp {
			fsl,pins = <
            MX6QDL_PAD_SD1_CMD__GPIO1_IO18       0x80000000  // {{external-gpio-5}} buzzer
			>;
		};		
// cndi button
		pinctrl_peributton: buttongrp {
			fsl,pins = <
            MX6QDL_PAD_GPIO_18__GPIO7_IO13       0x80000000  // {{external-gpio-39}} (KEY_VOL_UP)
            MX6QDL_PAD_NANDF_D0__GPIO2_IO00      0x80000000  // {{external-gpio-40}} (HOME)
            MX6QDL_PAD_NANDF_D3__GPIO2_IO03      0x80000000  // {{external-gpio-41}} (SEARCH)
            MX6QDL_PAD_NANDF_D2__GPIO2_IO02      0x80000000  // {{external-gpio-42}} (BACK)
            MX6QDL_PAD_NANDF_D1__GPIO2_IO01      0x80000000  // {{external-gpio-43}} (MENU)
            MX6QDL_PAD_GPIO_19__GPIO4_IO05       0x80000000  // {{external-gpio-44}} (KEY_VOL_DOWN)
			>;
		};		
		
		
        pinctrl_flexcan1: can1grp {
            fsl,pins = <
            MX6QDL_PAD_GPIO_7__FLEXCAN1_TX    0x1b0b1
            MX6QDL_PAD_GPIO_8__FLEXCAN1_RX    0x1b0b1
            >;
        };

        pinctrl_ecspi1: ecspi1grp {
            fsl,pins = <
            MX6QDL_PAD_DISP0_DAT22__ECSPI1_MISO  0x100b1
            MX6QDL_PAD_DISP0_DAT20__ECSPI1_SCLK  0x100b1
            MX6QDL_PAD_DISP0_DAT21__ECSPI1_MOSI  0x100b1
            MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17   0x80000000
            >;
        };

        pinctrl_ecspi2: ecspi2grp {
            fsl,pins = <
            MX6QDL_PAD_EIM_OE__ECSPI2_MISO      0x100b1
            MX6QDL_PAD_EIM_CS0__ECSPI2_SCLK     0x100b1
            MX6QDL_PAD_EIM_CS1__ECSPI2_MOSI     0x100b1
	    MX6QDL_PAD_EIM_D24__GPIO3_IO24       0x80000000  // {{external-gpio-53}}
//            MX6QDL_PAD_DISP0_DAT18__GPIO5_IO12  0x80000000
            >;
        };

        pinctrl_usdhc1: usdhc1grp {
            fsl,pins = <
            MX6QDL_PAD_SD1_CMD__SD1_CMD       0x17059
            MX6QDL_PAD_SD1_CLK__SD1_CLK       0x10059
            MX6QDL_PAD_SD1_DAT0__SD1_DATA0    0x17059
            MX6QDL_PAD_SD1_DAT1__SD1_DATA1    0x17059
            MX6QDL_PAD_SD1_DAT2__SD1_DATA2    0x17059
            MX6QDL_PAD_SD1_DAT3__SD1_DATA3    0x17059
            >;
        };

        pinctrl_pwm1: pwm1grp {
            fsl,pins = <
            MX6QDL_PAD_SD1_DAT3__PWM1_OUT     0x1b0b1  // {{external-pwm1-8}}
//          MX6QDL_PAD_GPIO_9__PWM1_OUT       0x1b0b1  // {{external-pwm1-11}}
            >;
        };
        
        pinctrl_pwm2: pwm2grp {
            fsl,pins = <
            MX6QDL_PAD_SD1_DAT2__PWM2_OUT     0x1b0b1  // {{external-pwm2-9}}
//          MX6QDL_PAD_GPIO_1__PWM2_OUT       0x1b0b1  // {{external-pwm2-10}}
            >;
        };
        
        pinctrl_pwm3: pwm3grp {
            fsl,pins = <
            MX6QDL_PAD_SD1_DAT1__PWM3_OUT     0x1b0b1  // {{external-pwm3-4}}
//          MX6QDL_PAD_SD4_DAT1__PWM3_OUT     0x1b0b1  // {{external-pwm3-6}}
            >;
        };
        
        pinctrl_pwm4: pwm4grp {
            fsl,pins = <
            MX6QDL_PAD_SD1_CMD__PWM4_OUT      0x1b0b1  // {{external-pwm4-5}}
//          MX6QDL_PAD_SD4_DAT2__PWM4_OUT     0x1b0b1  // {{external-pwm4-7}}
            >;
        };

        pinctrl_spdif: spdifgrp {
            fsl,pins = <
            MX6QDL_PAD_GPIO_19__SPDIF_OUT     0x1b0b0
            MX6QDL_PAD_EIM_D21__SPDIF_IN      0x1b0b0
            >;
        };

        pinctrl_uart3: uart3grp {
            fsl,pins = <
            MX6QDL_PAD_EIM_D24__UART3_TX_DATA      0x1b0b1
            MX6QDL_PAD_EIM_D25__UART3_RX_DATA      0x1b0b1
            >;
        };
        
        pinctrl_uart5: uart5grp {
            fsl,pins = <
            MX6QDL_PAD_CSI0_DAT14__UART5_TX_DATA      0x1b0b1
            MX6QDL_PAD_CSI0_DAT15__UART5_RX_DATA      0x1b0b1
            >;
        };
    };
 };

&flexcan1 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_flexcan1>;
    status = "disabled";
};

 
&usdhc1 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_usdhc1>;
    non-removable;
    keep-power-in-suspend;
    no-1-8-v;
    enable-sdio-wakeup;
    status = "disabled";
};

&pwm1 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_pwm1>;
    status = "okay";
};

&pwm2 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_pwm2>;
    status = "okay";
};

&pwm3 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_pwm3>;
    status = "okay";
};

&pwm4 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_pwm4>;
    status = "disabled";
};

&spdif {
   pinctrl-names = "default";
   pinctrl-0 = <&pinctrl_spdif>;
   status = "disabled";
};

&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3>;
	status = "disabled";
};

&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5>;
	status = "disabled";
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";
        accelerometer: fxos8700@1e {
                compatible = "fsl,fxos8700";
                reg = <0x1e>;
        };

        gyroscope: fxas2100x@20 {
                compatible = "fsl,fxas2100x";
                reg = <0x20>;
        };

};

&ecspi1 {
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio5 17 0>;
	pinctrl-0 = <&pinctrl_ecspi1>;
	pinctrl-names = "default";
	status = "disabled";

	spidev0: spi@0 {
		compatible = "spidev";
		reg = <0>;
		spi-max-frequency = <2000000>;
	};
};

&ecspi2 {
	fsl,spi-num-chipselects = <1>;
	// cs-gpios = <&gpio5 12 0>;
	cs-gpios = <&gpio3 24 0>; //D53
	pinctrl-0 = <&pinctrl_ecspi2>;
	pinctrl-names = "default";
//	status = "disabled";
	status = "okay";

	spidev1: spi@0 {
		compatible = "spidev";
		reg = <0>;
		spi-max-frequency = <2000000>;
	};
/*
	spidev1: spi@0 {
		compatible = "lm74";
		reg = <0>;
		spi-max-frequency = <2000000>;
	};
*/
};

/ {
    sound_spdif {
        compatible = "fsl,imx-audio-spdif";
        model = "imx-spdif";
        spdif-controller = <&spdif>;
        spdif-out;
        spdif-in;
        status = "disabled";
    };

    periled {
	compatible = "cndi,periled";
        pinctrl-names = "default";  
        pinctrl-0 = <&pinctrl_periled>;
		periled-gpio-0 = <&gpio5 16 GPIO_ACTIVE_HIGH>;
		periled-gpio-1 = <&gpio5 17 GPIO_ACTIVE_HIGH>;
		periled-gpio-2 = <&gpio3 25 GPIO_ACTIVE_HIGH>;
		periled-gpio-3 = <&gpio1 20 GPIO_ACTIVE_HIGH>;
		periled-gpio-4 = <&gpio1 16 GPIO_ACTIVE_HIGH>;
//		periled-gpio-5 = <&gpio1 17 GPIO_ACTIVE_HIGH>;
		periled-gpio-5 = <&gpio2 9 GPIO_ACTIVE_HIGH>;
		periled-gpio-6 = <&gpio2 10 GPIO_ACTIVE_HIGH>;
		periled-gpio-7 = <&gpio1 1 GPIO_ACTIVE_HIGH>;
        status = "okay"; 
    };
	
	peritextlcd {
	compatible = "cndi,peritextlcd";
        pinctrl-names = "default";  
        pinctrl-0 = <&pinctrl_peritextlcd>;
		perilcd-gpip-rs = <&gpio5 5 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-rw = <&gpio5 6 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-e = <&gpio5 7 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d0 = <&gpio5 8 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d1 = <&gpio5 9 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d2 = <&gpio5 10 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d3 = <&gpio5 11 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d4 = <&gpio5 12 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d5 = <&gpio5 13 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d6 = <&gpio5 14 GPIO_ACTIVE_HIGH>;
		perilcd-gpio-d7 = <&gpio5 15 GPIO_ACTIVE_HIGH>;
        status = "okay"; 
	};

	perifnd {
	compatible = "cndi,perifnd";
        pinctrl-names = "default";  
        pinctrl-0 = <&pinctrl_perifnd>;
		perifnd-gpio-a = <&gpio4 27 GPIO_ACTIVE_HIGH>;  // {{external-gpio-22}} FND_D0  FND_a
		perifnd-gpio-b = <&gpio4 28 GPIO_ACTIVE_HIGH>;  // {{external-gpio-23}} FND_D1  FND_b
		perifnd-gpio-c = <&gpio4 8 GPIO_ACTIVE_HIGH>;   // {{external-gpio-49}} FND_D7  FND_c
		perifnd-gpio-d = <&gpio4 9 GPIO_ACTIVE_HIGH>;   // {{external-gpio-48}} FND_D6  FND_d
		perifnd-gpio-e = <&gpio4 30 GPIO_ACTIVE_HIGH>;  // {{external-gpio-25}} FND_D4  FND_e
		perifnd-gpio-f = <&gpio4 31 GPIO_ACTIVE_HIGH>;  // {{external-gpio-26}} FND_D5  FND_f
		perifnd-gpio-g = <&gpio4 29 GPIO_ACTIVE_HIGH>;  // {{external-gpio-24}} FND_D3  FND_g
		perifnd-gpio-dp = <&gpio2 22 GPIO_ACTIVE_HIGH>; // {{external-gpio-38}} FND_D2  FND_DP
		
		perifnd-gpio-s0 = <&gpio5 31 GPIO_ACTIVE_HIGH>;
		perifnd-gpio-s1 = <&gpio5 30 GPIO_ACTIVE_HIGH>;
		perifnd-gpio-s2 = <&gpio6 1 GPIO_ACTIVE_HIGH>;
		perifnd-gpio-s3 = <&gpio6 0 GPIO_ACTIVE_HIGH>;
		perifnd-gpio-s4 = <&gpio6 2 GPIO_ACTIVE_HIGH>;
		perifnd-gpio-s5 = <&gpio5 22 GPIO_ACTIVE_HIGH>;
        status = "okay"; 
	};
	peribuzzer {
	compatible = "cndi,peribuzzer";
        pinctrl-names = "default";  
        pinctrl-0 = <&pinctrl_peribuzzer>;
		peribuzzer-gpio = <&gpio1 18 GPIO_ACTIVE_HIGH>;  // {{external-gpio-5}} buzzer	
        status = "okay"; 
	};

	peributton {
	compatible = "cndi,peributton";
        pinctrl-names = "default";  
        pinctrl-0 = <&pinctrl_peributton>;
